<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
  <context>
    <name>Juice Shop Scan</name>
    <desc>Exclude third-party domains</desc>
    
    <!-- Include only your app -->
    <inscope>http://localhost:3000.*</inscope>
    
    <!-- Exclude Cloudflare CDN -->
    <exclude>https://cdnjs.cloudflare.com/.*</exclude>
    <exclude>http://cdnjs.cloudflare.com/.*</exclude>
    <exclude>https://challenges.cloudflare.com/.*</exclude>
    <exclude>https://static.cloudflareinsights.com/.*</exclude>
  </context>
</configuration>
